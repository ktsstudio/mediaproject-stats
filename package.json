{
  "name": "@ktsstudio/mediaproject-stats",
  "version": "1.0.0",
  "repository": "https://github.com/ktsstudio/mediaproject-stats.git",
  "author": "KTS Studio",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "keywords": [
    "ga",
    "ym",
    "top mail",
    "statistic",
    "snitch",
    "pixel"
  ],
  "scripts": {
    "rm-build": "rm -rf dist",
    "lint": "eslint src --fix",
    "codestyle": "prettier src --write \"*.{ts,js}?(x)\"",
    "tsc-check": "tsc --skipLibCheck --noEmit --project tsconfig.json",
    "build": "yarn run rm-build; tsc --module es6 --target es6 --outDir dist --declarationDir dist/types",
    "build:cjs": "yarn run rm-build; tsc --module commonjs --target es5 --outDir dist --declarationDir dist/types",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,js}?(x)": [
      "yarn run lint",
      "yarn run codestyle"
    ]
  },
  "files": [
    "dist/",
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "logo.png"
  ],
  "devDependencies": {
    "@types/node": "^16.4.5",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3",
    "@types/react": "^17.0.39",
    "react": "^17.0.2"
  },
  "dependencies": {
    "@vkontakte/vk-bridge": "^2.5.1",
    "axios": "^0.21.1",
    "whatwg-fetch": "^3.6.2"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "private": false
}
